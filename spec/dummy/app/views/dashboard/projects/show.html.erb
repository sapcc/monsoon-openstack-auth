<%= content_for :title do%>
  Project <%= @project.name %>
<%end%>

<%= link_to 'Back to organization', domain_path(@region,@domain_id) %>

<div class="project" style="padding: 15px">
  <table>
    <tbody>
      <tr>
        <td>Name</td>
        <td><%=@project.name%></td>
      </tr>
      <tr>    
        <td>Description</td>
        <td><%=@project.description%></td>
      </tr>
      <tr>  
        <td>enabled</td>
        <td><%=@project.enabled%></td>
      </tr>
    </tbody>
  </table>
</div>

<%= button_to 'TEST Edit action', domain_project_path(@region,@domain_id, @project_id), method: :put %>
<%= button_to 'TEST Destroy action', domain_project_path(@region,@domain_id, @project_id), method: :delete %>
